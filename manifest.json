{
  	"manifest_version": 2,

  	"name": "Save The Session",
	"description": "This extension helps you to save your the tabs in your current Chrome session, and reload them later.",
	"key": "",
	"version": "1.0",

	"browser_action": {
		"default_icon": "assets/STS.png",
	    "default_popup": "popup.html"
	},
	"permissions": [
	    "tabs",
	    "storage",
	    "unlimitedStorage"
	]
}
